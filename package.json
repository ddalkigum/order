{
  "name": "order",
  "version": "1.0.0",
  "private": true,
  "main": "dist",
  "scripts": {
    "build": "rimraf ./dist && tsc --build",
    "start": "npm run build && export NODE_ENV=production && node ./dist/server.js",
    "start:dev": "export NODE_ENV=development && pm2 start ecosystem.config.js --env env_development",
    "start:local": "export NODE_ENV=local && nodemon ./src/server.ts",
    "docker:start": "docker-compose up -d",
    "typeorm": "ts-node node_modules/typeorm/cli -f src/infrastructure/db/mariadb/ormconfig.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,ts}'",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "author": "Ddalkigum",
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.1.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "debug": "~2.6.9",
    "dotenv": "^9.0.2",
    "elasticsearch": "^16.7.2",
    "express": "~4.16.1",
    "express-winston": "^4.1.0",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.3",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34",
    "typeorm-encrypted": "^0.5.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/debug": "^4.1.6",
    "@types/elasticsearch": "^5.0.38",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.18.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.0-1",
    "nodemon": "^2.0.7",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.0.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{ts, js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
